  "betterScripts": {
    "dev": {
      "command": "concurrently --kill-others \"nodemon node server.js\" ",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "test": {
      "command": "nyc --reporter=html --reporter=text --reporter=cobertura --exclude \"node_modules/**/*\" mocha \"./services/**/*.test.js\" --timeout 10000 --retries 5 --require ./test/setting.cjs --exit",
